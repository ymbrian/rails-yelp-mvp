<div class="container text-center">
  <div class="row">
    <div class="col-xs-12 restaurant-name">
      <% @restaurants.each do |restaurant| %>
        <h2><%= link_to restaurant.name, restaurant_path(restaurant) %></h2>
        <p><%= restaurant.category.capitalize %></p>
      <% end %>
    </div>
  </div>
</div>

